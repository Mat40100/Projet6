    {% if tricks == null %}
        <div class="col-12 row align-items-center justify-content-center">
            <div class="text-center">Pas de tricks publi√©s</div>
        </div>
    {% endif %}

    {% for trick in tricks %}
        <div class="Trick col-10 col-sm-6 col-md-5 col-lg-3 col-xl-2 p-4 row mb-4 m-xl-1">
            <a href="{{ path('app_trick_view', {'trick':trick.id,'slug':trick.name|slugify}) }}" class="col-12 tricks row justify-content-center align-items-center p-0 rounded">
                <img class="col-12 img-trick img-fluid m-0 p-0" src="{{ asset(trick.hasMainMedia) }}">
                <div class="link-tricks align-self-end col-12 text-center align-items-center row p-0">
                    <div class="trick-name col-12 p-0">{{ trick.name }}</div>
                </div>
            </a>
        </div>
    {%  endfor %}

    <div class="col-12 row justify-content-center align-items-baseline p-2">
        {% if loaded is defined and loaded > 10 %}
            <a href="#Landpage" class="position-absolute d-flex arrow justify-content-center align-items-center" href="#Tricks"><i class="fas fa-arrow-up"></i></a>
        {% endif %}
        {% if tricksLeft %}
            <a id="loadMore" class="col-auto text-center p-3 rounded" >Voir plus</a>
        {% else %}
        {% endif %}
    </div>

    {% block js %}
	 <script type="text/javascript" src="{{ asset('build/indexJs.js') }}"></script>
    {%endblock %}
